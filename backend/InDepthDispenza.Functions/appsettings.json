{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Function": "Trace",
      "Function.InDepthDispenza": "Trace",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CosmosDb": {
    "DatabaseName": "indepth-dispenza",
    "TranscriptCacheContainer": "transcript-cache",
    "VideoAnalysisContainer": "video-analysis",
    "TaxonomyVersionsContainer": "taxonomy-versions"
  },
  "AzureOpenAI": {
    "DeploymentName": "gpt-4o-mini",
    "ApiVersion": "2024-02-01"
  },
  "Grok": {
    "Enabled": true,
    "BaseUrl": "https://api.x.ai/v1",
    "Model": "grok-4-1-fast-reasoning",
    "MaxTokens": 4000,
    "Temperature": 0.7
  },
  "YouTubeTranscriptApi": {
    "BaseUrl": "https://www.youtube-transcript.io"
  },
  "VideoAnalysis": {
    "PreferredLanguages": ["en"]
  }
}
